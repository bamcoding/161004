    <servlet-class>net.gondor.users.web.DoJoinServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DoJoinServlet</servlet-name>
    <url-pattern>/doJoin</url-pattern>
  </servlet-mapping>
